<window type="dialog" id="1972">
	<defaultcontrol>10</defaultcontrol>
	<previouswindow>1997</previouswindow>
	<animation effect="fade" time="200">WindowOpen</animation>
	<animation effect="fade" delay="1050" time="50">WindowClose</animation>
	<controls>
		<include>TheDialogs</include>
		<control type="image">
			<description>Divider</description>
			<posx>370</posx>
			<posy>336</posy>
			<width>300</width>
			<height>1</height>
			<texture>divider.png</texture>
			<animation effect="fade" delay="450" time="200">WindowOpen</animation>
			<animation effect="fade" time="200">WindowClose</animation>
		</control>
		<control type="image">
			<description>Divider</description>
			<posx>370</posx>
			<posy>373</posy>
			<width>300</width>
			<height>1</height>
			<texture>divider.png</texture>
			<animation effect="fade" delay="450" time="200">WindowOpen</animation>
			<animation effect="fade" time="200">WindowClose</animation>
		</control>
		<control type="image">
			<description>Divider</description>
			<posx>370</posx>
			<posy>410</posy>
			<width>300</width>
			<height>1</height>
			<texture>divider.png</texture>
			<animation effect="fade" delay="450" time="200">WindowOpen</animation>
			<animation effect="fade" time="200">WindowClose</animation>
		</control>
		<control type="image">
			<description>Alert Icon</description>
			<posx>370</posx>
			<posy>40</posy>
			<width>30</width>
			<height>30</height>
			<texture>icon-info.png</texture>
			<animation effect="fade" delay="450" time="200">WindowOpen</animation>
			<animation effect="fade" time="200">WindowClose</animation>
		</control>
		<control type="label" id="1">
			<description>Launch Media Center</description>
			<posx>410</posx>
			<posy>40</posy>
			<width>303</width>
			<label>Launch Media Center</label>
			<align>left</align>
			<include>DialogTitle</include>
			<animation effect="fade" delay="450" time="200">WindowOpen</animation>
			<animation effect="fade" time="200">WindowClose</animation>
		</control>
		<control type="label" id="4">
			<description>dialog line</description>
			<posx>380</posx>
			<posy>115</posy>
			<width>320</width>
			<label>This will leave XBMC and launch Media Center Extender.  Any currently playing media will be stopped.  Are you sure you want to continue?</label>
			<font>font14</font>
			<wrapmultiline>true</wrapmultiline>
			<include>TextcolorBlack</include>
			<include>shadow-white</include>
			<animation effect="fade" delay="450" time="200">WindowOpen</animation>
			<animation effect="fade" time="200">WindowClose</animation>
		</control>
		<control type="button" id="11">
			<description>Yes Button</description>
			<posx>340</posx>
			<posy>326</posy>
			<width>350</width>
			<height>59</height>
			<texturefocus>guide-button-focus.png</texturefocus>
			<texturenofocus>-</texturenofocus>
			<pulseonselect>no</pulseonselect>
			<onclick>XBMC.RunXBE($INFO[Skin.String(MediaCenterPath)]\default.xbe)</onclick>
			<onclick>XBMC.Dialog.Close(1997)</onclick>
			<onleft>11</onleft>
			<onright>11</onright>
			<onup>10</onup>
			<ondown>10</ondown>
			<font>font14</font>
			<label>107</label>
			<textoffsetx>40</textoffsetx>
			<include>TextcolorBlack</include>
			<include>shadow-white</include>
			<animation effect="fade" delay="450" time="200">WindowOpen</animation>
			<animation effect="fade" time="200">WindowClose</animation>
			<animation effect="zoom" start="95" end="100" center="515,356" time="300">focus</animation>
		</control>
		<control type="button" id="10">
			<description>No Button</description>
			<posx>340</posx>
			<posy>363</posy>
			<width>350</width>
			<height>59</height>
			<texturefocus>guide-button-focus.png</texturefocus>
			<texturenofocus>-</texturenofocus>
			<pulseonselect>no</pulseonselect>
			<onclick>XBMC.Dialog.Close(1997)</onclick>
			<onleft>10</onleft>
			<onright>10</onright>
			<onup>11</onup>
			<ondown>11</ondown>
			<font>font14</font>
			<label>106</label>
			<textoffsetx>40</textoffsetx>
			<include>TextcolorBlack</include>
			<include>shadow-white</include>
			<animation effect="fade" delay="450" time="200">WindowOpen</animation>
			<animation effect="fade" time="200">WindowClose</animation>
			<animation effect="zoom" start="95" end="100" center="515,393" time="300">focus</animation>
		</control>
	</controls>
</window>